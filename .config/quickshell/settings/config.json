{
  "appearance": {
    "extraBackgroundTint": true,
    "fakeScreenRounding": 0,
    "isDarkMode": true,
    "palette": {
      "type": "auto"
    },
    "transparency": false,
    "wallpaperTheming": {
      "enableAppsAndShell": true,
      "enableQtApps": true,
      "enableTerminal": false
    }
  },
  "apps": {
    "bluetooth": "blueman-manager",
    "network": "plasmawindowed org.kde.plasma.networkmanagement",
    "networkEthernet": "kcmshell6 kcm_networkmanagement",
    "taskManager": "wezterm -e btop %F",
    "terminal": "kitty -1"
  },
  "audio": {
    "protection": {
      "enable": false,
      "maxAllowed": 90,
      "maxAllowedIncrease": 10
    }
  },
  "background": {
    "clockX": -500,
    "clockY": -500,
    "fixedClockPosition": false,
    "parallax": {
      "enableSidebar": false,
      "enableWorkspace": false,
      "workspaceZoom": 1.1
    },
    "wallpaperPath": "/home/gmartins/Pictures/Wallpapers/wallpaper.jpg"
  },
  "bar": {
    "borderless": true,
    "bottom": false,
    "cornerStyle": 2,
    "resources": {
      "alwaysShowCpu": false,
      "alwaysShowSwap": true
    },
    "screenList": [],
    "showBackground": true,
    "topLeftIcon": "distro",
    "tray": {
      "ignoredClasses": [
        "xwaylandvideobridge",
        "blueman",
        "Stremio",
        "Polychromatic"
      ],
      "monochromeIcons": false
    },
    "utilButtons": {
      "showColorPicker": false,
      "showDarkModeToggle": false,
      "showKeyboardToggle": false,
      "showMicToggle": false,
      "showPerformanceProfileToggle": false,
      "showScreenSnip": false
    },
    "verbose": true,
    "weather": {
      "city": "",
      "enable": false,
      "enableGPS": true,
      "fetchInterval": 10,
      "useUSCS": false
    },
    "workspaces": {
      "alwaysShowNumbers": true,
      "monochromeIcons": true,
      "showAppIcons": false,
      "showNumberDelay": 300,
      "shown": 10
    }
  },
  "battery": {
    "automaticSuspend": false,
    "critical": 5,
    "low": 20,
    "suspend": 3
  },
  "dock": {
    "enable": true,
    "height": 60,
    "hoverRegionHeight": 2,
    "hoverToReveal": true,
    "ignoredAppRegexes": [],
    "monochromeIcons": false,
    "pinnedApps": [
      "firefox",
      "org.keepassxc.keepassxc"
    ],
    "pinnedOnStartup": false
  },
  "hacks": {
    "arbitraryRaceConditionDelay": 20
  },
  "language": {
    "translator": {
      "engine": "auto",
      "sourceLanguage": "auto",
      "targetLanguage": "auto"
    }
  },
  "light": {
    "night": {
      "automatic": false,
      "colorTemperature": 5000,
      "from": "19:00",
      "to": "06:30"
    }
  },
  "networking": {
    "userAgent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/123.0.0.0 Safari/537.36"
  },
  "osd": {
    "timeout": 1000
  },
  "osk": {
    "layout": "qwerty_full",
    "pinnedOnStartup": false
  },
  "overview": {
    "columns": 5,
    "rows": 2,
    "scale": 0.18
  },
  "resources": {
    "updateInterval": 3000
  },
  "screenshotTool": {
    "showContentRegions": true
  },
  "search": {
    "engineBaseUrl": "https://www.google.com/search?q=",
    "excludedSites": [
      "quora.com"
    ],
    "nonAppResultDelay": 30,
    "prefix": {
      "action": "/",
      "clipboard": ";",
      "emojis": ":"
    },
    "sloppy": true
  },
  "sidebar": {
    "booru": {
      "allowNsfw": false,
      "defaultProvider": "yandere",
      "limit": 20,
      "zerochan": {
        "username": "[unset]"
      }
    },
    "translator": {
      "delay": 300
    }
  },
  "time": {
    "dateFormat": "dd/MM/yyyy",
    "format": "hh:mm"
  },
  "windows": {
    "centerTitle": false,
    "showTitlebar": false
  }
}
